[ReleaseProperties]
; Unique mod ID. Leave blank for auto GUID (OriginalAuthor.Name)
GUID=

; The name of the mod, used for AutoGUID, FileName, and anything else which needs a file-friendly name. Leave blank to use existing data.
PluginName=DelayedShards

; User friendly name of the mod. Used for thunderstore and BepinPlugin names visible to users. Leave blank to use the PluginName.
UserPluginName=Delayed Shards

; Thunderstore mod name/page link. Must only have characters [a-z A-Z 0-9 _]. Leave Blank for autofill based on UserPluginName. " " is auto replaced with "_".
ThunderstorePluginName=Delayed_Shards

; The current version of the mod. Used for file version, BepinPlugin, and Thunderstore manifest.
PluginVersion=0.0.7

; Version of the game this mod is built for. Used for README
GameVersion=0.27.0

; The simple description of the mod, used for VoidManager and thunderstore manifest descriptions. Must be less than 250 Characters
PluginDescription=Stores functional shards when inserted into the astral map, which may be activated on demand. Session Mod.

; The original author of the mod, used for auto GUID.
PluginOriginalAuthor=18107

; The various authors/editors of the mod.
PluginAuthors=18107, Dragon

; WebpageLink - Used by manifest for thunderstore page. Can be left blank.
WebpageLink=https://github.com/Void-Crew-Modding-Team/DelayedShards

; ThunderStore ID (https://thunderstore.io/c/void-crew/p/VoidCrewModdingTeam/VoidManager/ the section equivelant to 'VoidCrewModdingTeam/VoidManager'). Leave blank if unknown.
ThunderstoreID=VoidCrewModdingTeam/Delayed_Shards

; Dependency Strings - Comma delineated, spacing or no spacing is okay. Used for README and manifest.
DependencyStrings= BepInEx-BepInExPack-5.4.2100, VoidCrewModdingTeam-VoidManager-1.2.2


; PreBuild Execution Params
[PrebuildExecParams]

; Throw error if icon.png does not exist.
IconError=true

; Throw error if CHANGELOG is not updated.
ChangelogError=true

; For output to project dir, use: "ProjectDir". For output to solution Dir, use: "SolutionDir". Leave blank to disable. All other input will use the given file path.
ProjectReadmeOutPath=SolutionDir

; Output a zip after building.
ZipOutput=true